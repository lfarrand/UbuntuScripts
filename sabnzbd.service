[Unit]
Description=SABnzbd Service
Wants=network-online.target
After=network-online.target

[Service]
User=lee
Group=lee
Type=forking
ExecStart=/usr/bin/sabnzbdplus --config-file /etc/sabnzbd/sabnzbd.ini --logging 1 --daemon
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
